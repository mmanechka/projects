   "metadata": {},
   "outputs": [],
   "source": [
    "def floor_value(row):\n",
    "    if row['floor'] == 1:\n",
    "        return '–ø–µ—Ä–≤—ã–π'\n",
    "    elif row['floor'] == row['floors_total']:\n",
    "        return '–ø–æ—Å–ª–µ–¥–Ω–∏–π'\n",
    "    else:\n",
    "        return '–¥—Ä—É–≥–æ–π'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "metadata": {},
   "outputs": [],
   "source": [
    "data['floor_value'] = data.apply(floor_value, axis=1)"
   ]
