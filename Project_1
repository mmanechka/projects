    "income_and_debt = data.pivot_table(index = 'determination_of_income', columns = 'debt', values = 'children', aggfunc = 'count')\n",
    "income_and_debt['ratio_of_return'] = (income_and_debt[0] / (income_and_debt[0] + income_and_debt[1]))*100\n",
    "income_and_debt['ratio_of_debt'] = (income_and_debt[1] / (income_and_debt[0] + income_and_debt[1]))*100\n",
    "\n",
    "income_and_debt"
